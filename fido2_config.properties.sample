# FIDO2 Client Simulator - Sample Configuration File
# Copy this file to fido2_config.properties and modify according to your needs

# Paths for credential storage
# You can use relative or absolute paths
keystore.path=/path/to/your/fido2_keystore.p12
metadata.path=/path/to/your/fido2_metadata.json

# Security settings
# Note: For production use, this should be changed and stored securely
keystore.password=your_secure_password

# Logging configuration
log.level=INFO
